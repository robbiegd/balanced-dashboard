{{#unless is_object_log }}

<h1 class="page-title">Logs
	{{#unless results.isLoaded}}<div class="loader-container"><span class="loader loading"></span></div>{{/unless}}
</h1>

{{else}}

{{#if is_object_log }}

<h3 class="page-title">Logs
    {{#unless results.isLoaded}}<div class="loader-container"><span class="loader loading"></span></div>{{/unless}}
</h3>

{{/unless}}

<div id="logs" class="results">
	{{view Balanced.LogsFiltersHeaderView}}

	<div class="items-wrapper">
		{{view Balanced.LogsResultsView}}
	</div>
</div>
